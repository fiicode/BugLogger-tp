{
  "name": "simple-electron-react",
  "productName": "Bugglogger TP",
  "version": "1.0.0",
  "description": "Simple boilerplate for building Electron apps with React",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds --overwrite --plateform=darwin --icon=assets/icon.icns",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiicode/BugLogger-tp.git"
  },
  "author": "bahaboubackr",
  "build": {
    "appId": "com.applyhead.pingo",
    "productName": "BugLogger TP",
    "copyright": "Copyright Â© 2020 fiistore",
    "mac": {
      "category": "public.app-category.developer-tools",
      "type": "distribution"
    },
    "publish": [
      "github"
    ]
  },
  "bugs": {
    "url": "https://github.com/fiicode/BugLogger-tp/issues"
  },
  "homepage": "https://github.com/fiicode/BugLogger-tp/blob/master/README.md",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "electron-updater": "^4.3.5",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.12",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.13.1",
    "react-moment": "^1.0.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
